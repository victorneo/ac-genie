{
    "name": "AC Genie",
    "description": "Build a Genie!",
    "version": "3.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": ["storage"],
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "genie.png",
            "32": "genie.png",
            "48": "genie.png",
            "128": "genie.png"
          }
    },
    "icons": {
        "16": "genie.png",
        "32": "genie.png",
        "48": "genie.png",
        "128": "genie.png"
    }
}